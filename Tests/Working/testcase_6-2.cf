print("Enter a letter:");
string l = input();

// Test IF, ELIF, ELSE statements and DOUBLE_EQUAL operator
if (l == "a") {
    print("You entered a");
}
elif (l == "b") {
    print("You entered b");
}
else {
    print("You didn't enter a or b");
}